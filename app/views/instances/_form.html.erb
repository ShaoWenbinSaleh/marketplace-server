<%= form_for(instance) do |f| %>
  <% if instance.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(instance.errors.count, "error") %> prohibited this instance from being saved:</h2>

      <ul>
      <% instance.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :platform_id %>
    <%= f.text_field :platform_id %>
  </div>

  <div class="field">
    <%= f.label :build_id %>
    <%= f.text_field :build_id %>
  </div>

  <div class="field">
    <%= f.label :launch_bindings %>
    <%= f.text_field :launch_bindings %>
  </div>

  <div class="field">
    <%= f.label :deployed_at %>
    <%= f.datetime_select :deployed_at %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
